body{font-family:var(--font)}p{line-height:1.5}h1,h2{margin:0}h1{font-size:32px;padding-bottom:0}@media screen and (min-width: 320px){h1{font-size:2rem}}@media screen and (min-width: 768px){h1{font-size:3rem}}@media screen and (min-width: 1024px){h1{font-size:4rem}}@media screen and (min-width: 1280px){h1{font-size:6rem}}@media screen and (min-width: 1440px){h1{font-size:8rem}}h2,.h2{font-size:22px;line-height:1}@media screen and (min-width: 320px){h2,.h2{font-size:1.5rem}}@media screen and (min-width: 768px){h2,.h2{font-size:2.25rem}}@media screen and (min-width: 1024px){h2,.h2{font-size:3rem}}:root{--container: 1600px;--black: #2e2e2e;--white: rgb(241, 241, 241);--navy: #28374d;--orange: #f59073;--pink: #eb4464;--mauve: #913e5f;--purple: #3e4066;--color-scheme-text-color: var(--white);--color-scheme-light-background: #909fb6;--color-scheme-background: var(--navy);--font: 'Cutive Mono', 'Courier New', monospace}*,*::after,*::before{box-sizing:border-box}::selection{color:var(--color-scheme-text-color);background:var(--color-scheme-background)}*:focus{outline:2px dotted var(--black)}html{line-height:1.15;-webkit-text-size-adjust:100%}body{position:relative;margin:0;color:var(--color-scheme-text-color);background-color:var(--color-scheme-background);font-size:18px}img{max-width:100%}a{color:inherit;text-decoration:none;transition:color 300ms ease-in-out, outline 300ms ease-in-out}@media (hover: hover){a:hover{outline:1px dotted var(--orange)}}.visually-hide{position:absolute;top:0;left:-9999px;clip:rec(0, 0, 0, 0);pointer-events:none}.unstyled-list{padding:0;margin:0}.unstyled-list li{list-style:none}.full-height{height:100vh}.center{text-align:center}.center-content{display:flex;justify-content:center;align-items:center}.animated{animation-duration:1s;animation-fill-mode:both}@keyframes fade-in{from{opacity:0}to{opacity:1}}.fade-in{animation-name:fade-in}.container{max-width:var(--container);margin:0 auto}.grid{display:grid}.site-header{padding:20px 0 100px}.site-header h1{color:var(--orange)}.site-header ul{display:flex;justify-content:center}.site-header ul li:not(:last-of-type){margin-right:20px}.site-header ul a{display:block;padding:20px;text-decoration:none}.site-header ul a[aria-current="page"]{outline:1px dashed var(--color-scheme-border)}.site-header__skip-link{position:fixed;top:-4em;left:50%;transform:translateX(-50%);transition:top .5s ease-in-out}.site-header__skip-link:focus{top:0}.footer{padding:100px 0 40px;text-align:center;font-size:12px}.hero{grid-template-columns:100px 1fr 100px}.hero__left{border-right:1px dashed var(--color-scheme-border);order:1}@media screen and (max-width: 767px){.hero__left{order:3;padding-top:15px;border-top:1px dashed var(--color-scheme-border);border-right:unset}}.hero__barcode{font-size:2.5rem}@media screen and (min-width: 768px){.hero__barcode{width:26px;margin-left:4px;writing-mode:vertical-rl}}.hero__center{grid-template-columns:100px 1fr;order:2;padding:80px 20px}@media screen and (max-width: 767px){.hero__center{padding:40px 20px}}.hero__stats{grid-template-columns:repeat(3, 1fr);grid-gap:1px;background-color:var(--color-scheme-border);border-top:1px solid var(--color-scheme-border)}.hero__stats div{padding:20px;background-color:var(--color-scheme-background)}.hero__right{order:3;border-left:1px dashed var(--color-scheme-border)}.hero__right span{font-size:2rem}@media screen and (min-width: 768px){.hero__right span{writing-mode:vertical-lr}}@media screen and (max-width: 767px){.hero__right span{font-size:1.2rem}}@media screen and (max-width: 767px){.hero__right{order:1;padding:20px;border-bottom:1px dashed var(--color-scheme-border);border-left:unset}}@media screen and (max-width: 767px){.hero{grid-template-columns:1fr}}.index-list a{color:var(--pink)}.index-list p{margin:0}.index-list__plant{margin:40px 0}.intro img{width:100%;height:100%;object-fit:cover}.intro p{margin-bottom:0}.intro__grid{grid-template-columns:repeat(auto-fit, minmax(375px, 1fr))}.intro__content{padding:80px;align-self:center}@media screen and (max-width: 1239px){.intro__content{padding:40px}}.pagination{padding:40px 0;background-color:var(--color-scheme-background);border-bottom:1px dashed var(--color-scheme-border)}.pagination ul{display:flex;flex-wrap:wrap;justify-content:center}.pagination li:not(:last-of-type){margin-right:10px}.pagination li[aria-current="page"]{outline:1px dashed var(--color-scheme-border)}.pagination li[aria-current="page"],.pagination a{padding:16px}.pagination a{display:block}

