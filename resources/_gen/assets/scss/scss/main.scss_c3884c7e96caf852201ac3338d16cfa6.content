body{font-family:var(--font);font-size:20px}p{line-height:1.5}h1,h2{margin:0}h1{font-size:32px;padding-bottom:0}@media screen and (min-width: 320px){h1{font-size:2rem}}@media screen and (min-width: 768px){h1{font-size:3rem}}@media screen and (min-width: 1024px){h1{font-size:4rem}}@media screen and (min-width: 1280px){h1{font-size:6rem}}@media screen and (min-width: 1440px){h1{font-size:10rem}}h2,.h2{font-size:22px;line-height:1}@media screen and (min-width: 320px){h2,.h2{font-size:1.5rem}}@media screen and (min-width: 768px){h2,.h2{font-size:2.25rem}}@media screen and (min-width: 1024px){h2,.h2{font-size:3rem}}:root{--container: 1200px;--black: #3d3d3d;--white: rgb(255, 255, 255);--color-scheme-text-color: var(--black);--color-scheme-light-background: #909fb6;--color-scheme-background: var(--white);--green: #749c74;--font: 'PT Sans', Arial, sans-serif}@media screen and (max-width: 1279px){:root{--container: 984px}}@media screen and (max-width: 1023px){:root{--container: 728px}}@media screen and (max-width: 767px){:root{--container: 375px}}@media screen and (max-width: 414px){:root{--container: 100%}}*,*::after,*::before{box-sizing:border-box}::selection{color:var(--color-scheme-text-color);background:var(--color-scheme-background)}*:focus{color:var(--green)}html{line-height:1.15;-webkit-text-size-adjust:100%}body{position:relative;margin:0;color:var(--color-scheme-text-color);background-color:var(--color-scheme-background);font-size:18px;padding:0 0 122px 0;min-height:100vh}img{max-width:100%}a{color:inherit;text-decoration:none;transition:color 300ms ease-in-out}@media (hover: hover){a:hover{color:var(--green)}}.visually-hide{position:absolute;top:0;left:-9999px;clip:rec(0, 0, 0, 0);pointer-events:none}.unstyled-list{padding:0;margin:0}.unstyled-list li{list-style:none}.full-height{height:100vh}.center{text-align:center}.center-content{display:flex;justify-content:center;align-items:center}.animated{animation-duration:1s;animation-fill-mode:both}@keyframes fade-in{from{opacity:0}to{opacity:1}}.fade-in{animation-name:fade-in}.blog a{text-decoration:underline;text-underline-offset:3px;display:block;margin:20px 0}.blog a:hover{color:var(--green)}.container{width:100%;max-width:var(--container);margin:0 auto;padding:50px 0}@media screen and (max-width: 414px){.container{padding:50px 20px}}.grid{display:grid}.grid-1-col{grid-template-columns:1fr;grid-gap:80px}@media screen and (max-width: 767px){.grid-1-col{grid-template-columns:1fr}}.grid-2-col{grid-template-columns:repeat(2, 1fr);grid-gap:80px}@media screen and (max-width: 767px){.grid-2-col{grid-template-columns:1fr}}.grid-3-col{grid-template-columns:repeat(3, 1fr);grid-gap:80px}@media screen and (max-width: 767px){.grid-3-col{grid-template-columns:1fr}}.site-header{position:fixed;top:0;right:0;bottom:0;background:var(--white);z-index:100000}.site-header ul{writing-mode:vertical-rl;display:flex;height:100%;justify-content:space-between;list-style:none;padding:0;margin:0}.site-header ul li{flex:1}.site-header ul a{display:block;padding:20px;text-align:center;text-decoration:none;text-transform:lowercase}@media screen and (max-width: 767px){.site-header ul a{padding:15px}}.site-header ul a:hover,.site-header ul a[aria-current="page"]{text-decoration:underline;text-underline-offset:2px}.site-header__skip-link{position:fixed;top:-4em;left:50%;transform:translateX(-50%);transition:top .5s ease-in-out}.site-header__skip-link:focus{top:0}.footer{padding:40px 0;text-align:center;font-size:12px;text-transform:lowercase;position:absolute;bottom:0;left:0;width:calc(100% - 106px)}@media screen and (max-width: 767px){.footer{width:100%}}.pagination{padding:40px 0;background-color:var(--color-scheme-background);border-bottom:1px dashed var(--color-scheme-border)}.pagination ul{display:flex;flex-wrap:wrap;justify-content:center}.pagination li:not(:last-of-type){margin-right:10px}.pagination li[aria-current="page"]{outline:1px dashed var(--color-scheme-border)}.pagination li[aria-current="page"],.pagination a{padding:16px}.pagination a{display:block}.plant-single img{width:200px;height:auto}.home-grid{grid-gap:40px;width:calc(100% - 106px)}@media screen and (max-width: 767px){.home-grid{width:calc(100% - 50px)}}.home-grid__left-column{display:flex;flex-direction:column;align-items:center}.home-grid__image{position:relative;height:70vh;width:100%}.home-grid__image img{width:100%;height:100%;object-fit:cover;object-position:center}.home-grid__gradient{position:absolute;top:0;left:0;right:0;bottom:0;background:-moz-linear-gradient(top, rgba(255,255,255,0) 0%, #fff 100%);background:-webkit-linear-gradient(top, rgba(255,255,255,0) 0%, #fff 100%);background:linear-gradient(to bottom, rgba(255,255,255,0) 0%, #fff 100%);filter:progid:DXImageTransform.Microsoft.gradient( startColorstr='#00ffffff', endColorstr='#ffffff',GradientType=0 )}.home-grid h1{writing-mode:vertical-lr;transform:rotate(180deg) translateX(50%);position:absolute;bottom:-100px;left:50%;z-index:2;font-size:rem(228);font-weight:400;line-height:120px}.home-grid__plant-list{max-width:300px;margin:150px auto}.home-blog{padding:300px 0 100px}.plant{position:relative;max-width:300px}.plant img{width:200px;height:200px;border-radius:50%}.plant a{position:absolute;width:100%;height:100%;top:0;left:0;right:0;bottom:0;text-transform:lowercase;white-space:nowrap;display:flex;align-items:center;padding:0 0 0 150px;z-index:2;text-shadow:0 0 7px #fff;transition:.2s all ease}.plant a:hover{padding:0 0 0 160px}.plant__gradient{position:absolute;top:0;left:0;width:200px;height:200px;background:-moz-linear-gradient(left, rgba(255,255,255,0) 40%, rgba(255,255,255,0.8) 100%);background:-webkit-linear-gradient(left, rgba(255,255,255,0) 40%, rgba(255,255,255,0.8) 100%);background:linear-gradient(to right, rgba(255,255,255,0) 40%, rgba(255,255,255,0.8) 100%);filter:progid:DXImageTransform.Microsoft.gradient( startColorstr='#00ffffff', endColorstr='#ffffff',GradientType=1 )}

